  },
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--max-old-space-size=8192 turbo build",
    "build:web:analyze": "turbo build:analyze",
    "build:web:antd": "pnpm run build --filter=@ittlr/web-antd",
    "build:web:docker": "./scripts/deploy/build-local-docker-image.sh",
    "build:web:docs": "pnpm run build --filter=@ittlr/docs",
    "build:web:ele": "pnpm run build --filter=@ittlr/web-ele",
    "build:web:naive": "pnpm run build --filter=@ittlr/web-naive",
    "build:web:play": "pnpm run build --filter=@ittlr/playground",
    "build:nest:nest-app": "nest build nest-app",
    "preview:nest:nest-app": "node dist/apps/nest-app/main",
    "clean": "node ./scripts/clean.mjs",
    "dev": "turbo-run dev",
    "dev:web:antd": "pnpm -F @ittlr/web-antd run dev",
    "dev:web:docs": "pnpm -F @ittlr/docs run dev",
    "dev:web:ele": "pnpm -F @ittlr/web-ele run dev",
    "dev:web:naive": "pnpm -F @ittlr/web-naive run dev",
    "dev:web:play": "pnpm -F @ittlr/playground run dev",
    "dev:nest:nest-app": "nest start --watch",
    "debug:nest:nest-app": "nest start --watch --debug",
    "preview": "turbo-run preview",
    "reinstall": "pnpm clean --del-lock && pnpm install",
    "test:unit": "vitest run --dom",
    "test:e2e": "turbo run test:e2e",
    "update:deps": "npx taze -r -w"
  },
  "devDependencies": {
    "@changesets/changelog-github": "catalog:",
