    "@ittlr/prettier-config": "workspace:*",
    "@ittlr/stylelint-config": "workspace:*",
    "@ittlr/tailwind-config": "workspace:*",
    "@ittlr/tsconfig": "workspace:*",
    "@ittlr/turbo-run": "workspace:*",
    "@ittlr/vite-config": "workspace:*",
    "@ittlr/vsh": "workspace:*",
    "@vitejs/plugin-vue": "catalog:",
    "@vitejs/plugin-vue-jsx": "catalog:",
    "@vue/test-utils": "catalog:",
